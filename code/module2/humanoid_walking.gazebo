<?xml version="1.0" ?>
<sdf version="1.7">
  <world name="humanoid_walking_world">
    <!-- Include basic environment -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Add a simple walking humanoid robot -->
    <model name="humanoid_robot">
      <!-- Base/Body Link -->
      <link name="base_link">
        <pose>0 0 1 0 0 0</pose>
        <inertial>
          <mass>10.0</mass>
          <inertia>
            <ixx>0.5</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.5</iyy>
            <iyz>0</iyz>
            <izz>0.5</izz>
          </inertia>
        </inertial>
        <visual name="base_visual">
          <geometry>
            <box>
              <size>0.3 0.2 0.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
          </material>
        </visual>
        <collision name="base_collision">
          <geometry>
            <box>
              <size>0.3 0.2 0.5</size>
            </box>
          </geometry>
        </collision>
      </link>

      <!-- Head -->
      <link name="head">
        <inertial>
          <mass>2.0</mass>
          <inertia>
            <ixx>0.01</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.01</iyy>
            <iyz>0</iyz>
            <izz>0.01</izz>
          </inertia>
        </inertial>
        <visual name="head_visual">
          <geometry>
            <sphere>
              <radius>0.1</radius>
            </sphere>
          </geometry>
        </visual>
        <collision name="head_collision">
          <geometry>
            <sphere>
              <radius>0.1</radius>
            </sphere>
          </geometry>
        </collision>
      </link>

      <joint name="neck_joint" type="fixed">
        <parent>base_link</parent>
        <child>head</child>
        <pose>0 0 0.4 0 0 0</pose>
      </joint>

      <!-- Left Leg -->
      <link name="left_thigh">
        <inertial>
          <mass>3.0</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.01</izz>
          </inertia>
        </inertial>
        <visual name="left_thigh_visual">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="left_thigh_collision">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <joint name="left_hip" type="revolute">
        <parent>base_link</parent>
        <child>left_thigh</child>
        <pose>-0.1 0.05 -0.25 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-1.57</lower>
            <upper>1.57</upper>
            <effort>100</effort>
            <velocity>2</velocity>
          </limit>
        </axis>
      </joint>

      <link name="left_shin">
        <inertial>
          <mass>2.5</mass>
          <inertia>
            <ixx>0.08</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.08</iyy>
            <iyz>0</iyz>
            <izz>0.01</izz>
          </inertia>
        </inertial>
        <visual name="left_shin_visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="left_shin_collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <joint name="left_knee" type="revolute">
        <parent>left_thigh</parent>
        <child>left_shin</child>
        <pose>0 0 -0.2 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>0</lower>
            <upper>2.36</upper>
            <effort>100</effort>
            <velocity>2</velocity>
          </limit>
        </axis>
      </joint>

      <link name="left_foot">
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.01</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.02</iyy>
            <iyz>0</iyz>
            <izz>0.02</izz>
          </inertia>
        </inertial>
        <visual name="left_foot_visual">
          <geometry>
            <box>
              <size>0.15 0.08 0.05</size>
            </box>
          </geometry>
        </visual>
        <collision name="left_foot_collision">
          <geometry>
            <box>
              <size>0.15 0.08 0.05</size>
            </box>
          </geometry>
        </collision>
      </link>

      <joint name="left_ankle" type="revolute">
        <parent>left_shin</parent>
        <child>left_foot</child>
        <pose>0 0 -0.2 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-0.79</lower>
            <upper>0.79</upper>
            <effort>50</effort>
            <velocity>1</velocity>
          </limit>
        </axis>
      </joint>

      <!-- Right Leg -->
      <link name="right_thigh">
        <inertial>
          <mass>3.0</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.01</izz>
          </inertia>
        </inertial>
        <visual name="right_thigh_visual">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="right_thigh_collision">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <joint name="right_hip" type="revolute">
        <parent>base_link</parent>
        <child>right_thigh</child>
        <pose>-0.1 -0.05 -0.25 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-1.57</lower>
            <upper>1.57</upper>
            <effort>100</effort>
            <velocity>2</velocity>
          </limit>
        </axis>
      </joint>

      <link name="right_shin">
        <inertial>
          <mass>2.5</mass>
          <inertia>
            <ixx>0.08</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.08</iyy>
            <iyz>0</iyz>
            <izz>0.01</izz>
          </inertial>
        </inertial>
        <visual name="right_shin_visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="right_shin_collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <joint name="right_knee" type="revolute">
        <parent>right_thigh</parent>
        <child>right_shin</child>
        <pose>0 0 -0.2 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>0</lower>
            <upper>2.36</upper>
            <effort>100</effort>
            <velocity>2</velocity>
          </limit>
        </axis>
      </joint>

      <link name="right_foot">
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.01</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.02</iyy>
            <iyz>0</iyz>
            <izz>0.02</izz>
          </inertial>
        </inertial>
        <visual name="right_foot_visual">
          <geometry>
            <box>
              <size>0.15 0.08 0.05</size>
            </box>
          </geometry>
        </visual>
        <collision name="right_foot_collision">
          <geometry>
            <box>
              <size>0.15 0.08 0.05</size>
            </box>
          </geometry>
        </collision>
      </link>

      <joint name="right_ankle" type="revolute">
        <parent>right_shin</parent>
        <child>right_foot</child>
        <pose>0 0 -0.2 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-0.79</lower>
            <upper>0.79</upper>
            <effort>50</effort>
            <velocity>1</velocity>
          </limit>
        </axis>
      </joint>

      <!-- Add Gazebo plugins for ROS 2 integration -->
      <plugin name="gazebo_ros_state" filename="libgazebo_ros_state.so">
        <ros>
          <namespace>/humanoid_robot</namespace>
        </ros>
        <publish_frequency>30</publish_frequency>
      </plugin>
    </model>

    <!-- Add a simple walking controller plugin -->
    <plugin name="humanoid_walking_controller" filename="libgazebo_ros_joint_pose_trajectory.so">
      <ros>
        <namespace>/humanoid_robot</namespace>
      </ros>
      <update_rate>100</update_rate>
    </plugin>

    <!-- Physics configuration -->
    <physics type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
      <ode>
        <solver>
          <type>quick</type>
          <iters>100</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
          <contact_surface_layer>0.001</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
  </world>
</sdf>